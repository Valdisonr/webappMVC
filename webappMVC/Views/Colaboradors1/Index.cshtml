@model IEnumerable<webappMVC.Colaborador>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Identificador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aceite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datacad)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Identificador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aceite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Datacad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idcolab">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idcolab">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idcolab">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
